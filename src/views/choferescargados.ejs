</head>

<body style="background: url(/imagenes/img-noise-1440x1080.png)">
    <!-- Encabezado -->
    <div class="container-encabezado-pagSecund">

        <!--NavBar boton inicio-->
        <div class="row">
            <div class="navbar-btnIni fixed-top dropdown mb-3">
                <button id="dropdownMenuBoton" type="button" class="btn menuIngreso-pagSecund">
                    <a href="/" ><b>Inicio</b></a>
                </button>
            </div>
        </div>
        <!--Fin NavBar boton inicio-->

        <!--Titulo y subtitulo-->
        <div class="fondo-encabezado-pagSecund" style="background: url(/imagenes/taxi.jpg)"></div>

        <div class="tituloysubtitulo-pagSecund">

            <div class="row">
                <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3"></div>
                <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
                    <div class="titulo-pagAdmin">
                        <p>Administrador</p>
                    </div>
                </div>
                <div class="col-xs-3 col-sm-3 col-md-3 col-lg-3"></div>
            </div>

            <div class="row">
                <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1"></div>
                <div class="col-xs-10 col-sm-10 col-md-10 col-lg-10">
                    <div class="subtitulo">
                        <p>Choferes cargados</p>
                    </div>
                </div>
                <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1"></div>
            </div>

        </div>
        <!--Fin Titulo y subtitulo-->

    </div>
    <!-- Fin Encabezado -->

    <!--Tabla de listado de choferes-->

    
        <div class="row-bruno centered">
            
                <table class="table table-bruno table-bordered">
                    <thead>
                        <tr>
                            <td><b>Nombre</b></td>
                            <td><b>Usuario</b></td>
                            <td><b>Password</b></td>
                            <td><b>X</b></td>
                        </tr>
                    </thead>
                    <tbody>
                    <% if (data) { %>
                        <% for(var i = 0; i < data.length; i++) { %>
                            <tr>
                                <td>
                                    <%= data[i].nombre %>
                                </td>
                                <td>
                                    <%= data[i].usuario %>
                                </td>
                                <td>
                                    <%= data[i].pass %>
                                </td>
                            </tr>
                        <% } %>
                    <% } %>
                    <% if (data == "") { %>
                        <tr>                            
                            <td id="celdaunida" colspan="3"><b>Usted aun no posee choferes cargados</b></td>                                                         
                        </tr>
                    <% } %>
                    </tbody>
                </table>                
                <form id="form-login" action="/redirCargasyConsultas" method="post">                   
                    <div class="form-group">
                        <input type="text" class="form-control" name="admin" value="<%=admin.admin %>" style="display: none">
                    </div>    
                    <button type="submit" id="btn-login" class="btn btn-volver btn-login btn-primary">
                        <b>Volver</b>
                    </button>                       
                </form>
            
        </div>
   

    <!--Fin tabla de listado de choferes-->

    <!--Footer-->
    <div class="containerFooter">
        <footer class="page-footer font-small blue">

            <!-- Copyright -->
            <div class="footer-copyright text-center">Â© 2020 Copyright:
                <a href="#"> By Bruno Aggazzani</a>
            </div>
            <!-- Copyright -->

        </footer>
    </div>
    <!--Fin Footer-->


